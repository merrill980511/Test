<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.merrill.onlinetest"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/th_topple5"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.QuestionActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/TitlebarStyle"></activity>
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/TitlebarStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.OrderActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/TitlebarStyle"></activity>
        <activity
            android:name=".ui.CollectActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/TitlebarStyle"></activity>
        <activity
            android:name=".ui.ExamActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/TitlebarStyle"></activity>
        <activity
            android:name=".ui.ResultActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/NoBiaoti"></activity>
        <activity
            android:name=".ui.ExamErrorActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/TitlebarStyle"></activity>
        <activity
            android:name=".ui.ErrorActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/TitlebarStyle"></activity>
        <activity
            android:name=".ui.HisResultActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/TitlebarStyle"></activity>
    </application>

</manifest>